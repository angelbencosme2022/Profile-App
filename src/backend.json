{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "patternProperties": {
      "^[a-zA-Z0-9]+$": {
        "type": "object",
        "properties": {
          "image": {
            "type": "string",
            "format": "uri"
          },
          "name": {
            "type": "string"
          }
        },
        "required": ["image", "name"],
        "additionalProperties": false
      }
    }
  }
  